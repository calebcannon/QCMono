<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>name</key>
		<string>C#</string>
		<key>syntaxDefinitionFile</key>
		<string>csharp.plist</string>
		<key>compileCommand</key>
		<string>gmcs %sourcefilepath% -target:library -d:input -d:output -out:%destfilepath%</string>
	</dict>
	<dict>
		<key>name</key>
		<string>Boo</string>
		<key>syntaxDefinitionFile</key>
		<string>boo.plist</string>
		<key>compileCommand</key>
		<string>booc -target:library -o:%destfilepath% %sourcefilepath%</string>
	</dict>
</array>
</plist>
